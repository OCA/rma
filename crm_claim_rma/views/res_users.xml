<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="res_users_rma_config_form_view" model="ir.ui.view">
            <field name="name">res.users.rma.config.form.view</field>
            <field name="model">res.users</field>
            <field name="inherit_id" ref="base.view_users_form"/>
            <field name="arch" type="xml">
                <xpath expr="//page[@string='Preferences']/group[@name='messaging']" position="before">
                    <group string="RMA" name="rma">
                        <field name="rma_warehouse_id"/>
                    </group>
                </xpath>
            </field>
        </record>

    </data>
</openerp>
