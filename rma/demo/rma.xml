<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <record id="rma_operation_replace" model="rma.operation">
        <field name="name">Replace</field>
        <field name="code">RPL</field>
        <field name="refund_type">no</field>
        <field name="shipment_type">ordered</field>
        <field name="delivery_type">received</field>
        <field name="route_customer" ref="rma.route_rma_customer"/>
        <field name="route_supplier" ref="rma.route_rma_supplier"/>
        <field name="is_dropship">False</field>
    </record>

    <record id="rma_operation_refund" model="rma.operation">
        <field name="name">Refund</field>
        <field name="code">RF</field>
        <field name="refund_type">ordered</field>
        <field name="shipment_type">no</field>
        <field name="delivery_type">no</field>
        <field name="route_customer" ref="rma.route_rma_customer"/>
        <field name="route_supplier" ref="rma.route_rma_supplier"/>
        <field name="is_dropship">False</field>
    </record>

    <record id="rma_operation_ds_replace" model="rma.operation">
        <field name="name">Replace with dropship</field>
        <field name="code">DSRPL</field>
        <field name="refund_type">no</field>
        <field name="shipment_type">ordered</field>
        <field name="delivery_type">received</field>
        <field name="route_customer" ref="rma.route_rma_dropship"/>
        <field name="route_supplier" ref="rma.route_rma_dropship"/>
        <field name="is_dropship">True</field>
    </record>
</odoo>
